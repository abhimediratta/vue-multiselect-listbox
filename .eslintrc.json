{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "prefer-arrow-callback": 0,
        "eqeqeq": 0,
        "no-param-reassign": 0,
        "max-len": 0,
        "linebreak-style": ["error", "unix"]
    }
}