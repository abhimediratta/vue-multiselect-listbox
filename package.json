{
  "name": "vue-multiselect-listbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Multi-select widget where users can select options from a list in the left side to a list on the right",
  "keywords": [
    "vue",
    "multiselect",
    "listbox",
    "dual",
    "selectbox"
  ],
  "main": "./dist/vue-multi-select-listbox.umd.js",
  "module": "./dist/vue-multi-select-listbox.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-multi-select-listbox.esm.js",
      "require": "./dist/vue-multi-select-listbox.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite ./dev --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2",
    "vue-tsc": "^1.8.27"
  },
  "homepage": "https://abhimediratta.github.io/vue-multiselect-listbox/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhimediratta/vue-multiselect-listbox.git"
  }
}
